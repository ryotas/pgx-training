{
  "vertex_uri_list": [
    "openflights.pgx.nodes"
  ],
  "edge_uri_list": [
    "openflights.pgx.edges"
  ],
  "format": "flat_file",
  "node_id_type": "string",
  "edge_label": true,
  "vertex_props": [
    {
      "name": "name",
      "type": "string"
    },
    {
      "name": "iata",
      "type": "string"
    },
    {
      "name": "city",
      "type": "string"
    },
    {
      "name": "latitude",
      "type": "double"
    },
    {
      "name": "longitude",
      "type": "double"
    }
  ],
  "edge_props": [
    {
      "name": "airline",
      "type": "string"
    },
    {
      "name": "country",
      "type": "string"
    },
    {
      "name": "plane",
      "type": "string"
    }
  ],
  "separator": ",",
  "loading": {
    "load_edge_label": true
  }
}